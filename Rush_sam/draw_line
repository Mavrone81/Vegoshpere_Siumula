#include <unistd.h>

void    ft_putchar(char c)
{
    write(1, &c, 1);
}

void   draw_line4(void)
{
    ft_putchar('1');
    ft_putchar('2');
    ft_putchar('3');
    ft_putchar('4');
    ft_putchar('\n');
}

void    draw_line3_1(char c, char d)
{
    ft_putchar('2');
    ft_putchar('c');
    ft_putchar('d');
    ft_putchar('4');
    ft_putchar('\n');
}

void    draw_line3_2(char c, char d, char e)
{
    ft_putchar('c');
    ft_putchar('d');
    ft_putchar('4');
    ft_putchar('e');
    ft_putchar('\n');
}

void    draw_line2_3 (char c, char d, char e)
{
    ft_putchar('c');
    ft_putchar('4');
    ft_putchar('d');
    ft_putchar('e');
    ft_putchar('\n');
}

void    draw_line1_2 (char c, char d)
{
    ft_putchar('4');
    ft_putchar('c');
    ft_putchar('d');
    ft_putchar('3');
    ft_putchar('\n');
}

void    draw_line1_3 (char c, char d)
{
    ft_putchar('4');
    ft_putchar('c');
    ft_putchar('d');
    ft_putchar('2');
    ft_putchar('\n');
}

void    draw_line1_4()
{
    ft_putchar('4');
    ft_putchar('3');
    ft_putchar('4');
    ft_putchar('1');
    ft_putchar('\n');
}